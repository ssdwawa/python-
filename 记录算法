//算素数
for i in range(2,100000):
    for j in range(2, int(math.sqrt(i)+1)):
        if (i % j ==0):
           break
    else:
        print i
        
        
//迭代
def fn (n):
    if n==1:
        return 1
    return fn(n-1) + n-2
def sum (n):
    if n==1:
        return 1
    return sum (n-1) + fn(n)
print  sum(2)
